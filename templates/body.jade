              li
                .spacer &nbsp;
              li
                button.btn-info.btn-xs.refresh
                  i.fa.fa-refresh(id='refreshIcon')
                button.btn-info.btn-xs.playPause
                  i.fa.fa-pause(id='playPauseIcon')
