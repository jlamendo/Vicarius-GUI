body
  .container
    .navbar.navbar-default.navLogo
     ul.nav.navbar-nav
      li
       a.vicariusLogo(href='#', data-autoHide='hide', id='navBarHideButton', onClick='window.toggleNavBar()')
        span.glyphicon.glyphicon-chevron-right
    .navbar.navbar-default.navMenu(id='rootNavbar')
            ul.nav.navbar-nav
              li
                a.navItem(href='Configuration') Configuration
              li
                a.navItem(href='IDE') IDE
              li
                a.navItem(href='History') History
              li
                .spacer &nbsp;
              li.selectBox
                select.selectpacker.profiles(data-style='btn-info btn-xs', data-show-icon='false', data-width='8.5em')
                  option(data-content="<p class='hoverEditContainer'><input role='createProfileInput' id='createProfileInput' class='editInput dropDown' type='text' placeholder='Create'><span class='hoverEditChild dropDown' id='selectedProfile'><i class='fa fa-plus'></i></span></p>")
              li
                .spacer &nbsp;
              li.selectBox
                select.selectpacker.projects(data-style='btn-info btn-xs', data-show-icon='false', data-width='8.5em')
